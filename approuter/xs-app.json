{	
	"welcomeFile": "/app/hanasapbtp/webapp/index.html",
	"authenticationMethod": "route",
	"sessionTimeout": 30,
	"logout": {
	"logoutEndpoint": "/do/logout",
	"logoutPage": "/"
   },
	"routes": [
   	{
	"source": "^/app/(.*)$",
	"target": "$1",
	"localDir": "resources"
	},     
	{
	"source": "/catalog/(.*)",
	"destination": "srv-binding"
	}
  ]
}